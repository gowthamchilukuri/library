{
  "compilerOptions": {
    "jsx": "react-jsx",  
    "module": "ESNext",
    "target": "ES5",
    "lib": ["dom", "esnext"],
    "declaration": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}
